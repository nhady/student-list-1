node{
  def app
  
  stage('Clone') {
    checkout scm
  }
  
  stage('Build image') {
    app = docker.build("my_image", "./simple_api/")
  }
  
  stage('Test image') {
    docker.image('my_image').withRun("--name myimage --network=default_default --link c11 --expose 7000 -p 7000:7000 -v /doc/student_age.json:/data/student_age.json"){c ->
      sh 'docker ps'
    
    }
  }
}
